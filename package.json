    "babel-core": "^7.0.0-bridge.0",
    "babel-jest": "^23.6.0",
    "babel-loader": "^8.0.4",
    "clean-webpack-plugin": "^1.0.0",
    "eslint": "^5.10.0",
    "jest": "^23.6.0",
    "nodemon": "^1.18.9",
    "prettier-eslint": "^8.8.2",
    "superagent": "^4.0.0",
    "supertest": "^3.3.0",
    "webpack": "^4.27.1",
    "webpack-cli": "^3.1.2",
    "webpack-node-externals": "^1.7.2"
